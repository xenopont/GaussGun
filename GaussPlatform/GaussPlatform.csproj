<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net10.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
        <UseWindowsForms>true</UseWindowsForms>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="Resources\Icons\TrayIcon\TrayIcon.ico" />
      <None Remove="Resources\Icons\TrayIcon\TrayIconWhite.ico" />
      <Resource Include="Resources\Icons\TrayIcon\TrayIconWhite.ico" />
    </ItemGroup>

    <ItemGroup>
      <_DeploymentManifestIconFile Remove="Assets\TrayIcon\TrayIcon.ico" />
    </ItemGroup>

</Project>
